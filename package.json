    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.2.6",
    "postcss": "^8.4.49",
    "prisma": "^6.9.0",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  }
